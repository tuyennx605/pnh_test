          "type": "string",
          "example": "chevrolet chevelle malibu"
        },
        "Miles_per_Gallon": {
          "type": "number",
          "example": "18"
        },
        "Cylinders": {
          "type": "number",
          "example": "8"
        },
        "Displacement": {
          "type": "number",
          "example": "307"
        },
        "Horsepower": {
          "type": "number",
          "example": "130"
        },
        "Weight_in_lbs": {
          "type": "number",
          "example": "3504"
        },
        "Acceleration": {
          "type": "number",
          "example": "12"
        },
        "Year": {
          "type": "string",
          "format": "date",
          "example": "1970-01-01"
        },
        "Origin": {
          "type": "string",
          "example": "USA"
        }
      }
    },
    "Name": {
      "properties": {
        "first": {
          "type": "string",
          "example": "Deidre"
        },
        "last": {
          "type": "string",
          "example": "Hayes"
        }
      }
    },
    "Account": {
      "properties": {
        "_id": {
          "type": "string",
          "example": 0
        },
        "name": {
          "$ref": "#/definitions/Name"
        },
        "email": {
          "type": "string",
          "example":"deidre.hayes@undefined.me"
        },
        "phone": {
          "type": "string",
          "example":"+1 (839) 577-3100"
        },
        "address": {
          "type": "string",
          "example": "507 Church Avenue, Heil, Wyoming, 1754"
        }
      }
